<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <LinearLayout
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:id="@+id/dialoginfolayoutLinearLayout0"
        android:paddingLeft="20.0dip"
        android:paddingTop="15.0dip"
        android:paddingBottom="5.0dip"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">
        <ImageView
            android:layout_width="30.0dip"
            android:layout_height="30.0dip"
            android:layout_marginRight="10.0dip"
            android:src="@drawable/color_about_96"
            android:scaleType="fitCenter" />
        <TextView
            android:textSize="18.0sp"
            android:textStyle="bold"
            android:textColor="#ff142fbc"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:hint="Information Library"
            android:text="" />
    </LinearLayout>
    <com.google.android.material.textfield.TextInputLayout
        android:orientation="vertical"
        android:id="@+id/dialoginfolayoutLinearLayout1"
        android:paddingLeft="20.0dip"
        android:paddingRight="20.0dip"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">
        <EditText
            android:textSize="18.0sp"
            android:textColorHint="#ff142fbc"
            android:id="@+id/edittext_info_name"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:hint="Name Library"
            android:text=""
            android:inputType="text"
            android:autofillHints="" />
    </com.google.android.material.textfield.TextInputLayout>
    <com.google.android.material.textfield.TextInputLayout
        android:orientation="vertical"
        android:id="@+id/dialoginfolayoutLinearLayout2"
        android:paddingLeft="20.0dip"
        android:paddingRight="20.0dip"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">
        <EditText
            android:textSize="18.0sp"
            android:textColorHint="#ff142fbc"
            android:id="@+id/edittext_info_import"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:hint="Import library name"
            android:text=""
            android:inputType="text"
            android:autofillHints="" />
    </com.google.android.material.textfield.TextInputLayout>
    <com.google.android.material.textfield.TextInputLayout
        android:orientation="vertical"
        android:id="@+id/dialoginfolayoutLinearLayout3"
        android:paddingLeft="20.0dip"
        android:paddingRight="20.0dip"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent">
        <ScrollView
            android:layout_width="fill_parent"
            android:layout_height="fill_parent">
        <EditText
            android:textSize="18.0sp"
            android:textColorHint="#ff142fbc"
            android:id="@+id/edittext_info_manifast"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:maxLines="6"
            android:hint="Manifast"
            android:text=""
            android:inputType="textMultiLine"
            android:autofillHints="" /></ScrollView>
    </com.google.android.material.textfield.TextInputLayout>
    <LinearLayout
        android:gravity="right"
        android:orientation="horizontal"
        android:background="#FF142FBC"
        android:paddingTop="10.0dip"
        android:paddingRight="18.0dip"
        android:paddingBottom="10.0dip"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        tools:ignore="RtlHardcoded">
        <TextView
            android:textStyle="bold"
            android:textColor="#ffffffff"
            android:id="@+id/text_info_ok"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15.0dip"
            android:text="@string/common_word_ok" />
    </LinearLayout>
</LinearLayout>