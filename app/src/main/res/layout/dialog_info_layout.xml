<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/dialoginfolayoutLinearLayout0"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:paddingLeft="20.0dip"
        android:paddingTop="15.0dip"
        android:paddingBottom="5.0dip">

        <ImageView
            android:layout_width="30.0dip"
            android:layout_height="30.0dip"
            android:layout_marginRight="10.0dip"
            android:scaleType="fitCenter"
            android:src="@drawable/color_about_96" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:hint="Information Library"
            android:text=""
            android:textColor="?colorPrimaryDark"
            android:textSize="18.0sp"
            android:textStyle="bold" />
    </LinearLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/dialoginfolayoutLinearLayout1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:paddingLeft="20.0dip"
        android:paddingRight="20.0dip">

        <EditText
            android:id="@+id/edittext_info_name"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:autofillHints=""
            android:editable="false"
            android:hint="Name Library"
            android:inputType="none"
            android:text=""
            android:textColorHint="?colorPrimaryDark"
            android:textIsSelectable="true"
            android:textSize="18.0sp"
            tools:ignore="Deprecated" />
    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/dialoginfolayoutLinearLayout2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:paddingLeft="20.0dip"
        android:paddingRight="20.0dip">

        <EditText
            android:id="@+id/edittext_info_import"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:autofillHints=""
            android:editable="false"
            android:hint="Import library name"
            android:inputType="none"
            android:text=""
            android:textColorHint="?colorPrimaryDark"
            android:textIsSelectable="true"
            android:textSize="18.0sp"
            tools:ignore="Deprecated" />
    </com.google.android.material.textfield.TextInputLayout>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/dialoginfolayoutLinearLayout3"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:paddingLeft="20.0dip"
            android:paddingRight="20.0dip">

            <EditText
                android:id="@+id/edittext_info_manifast"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:autofillHints=""
                android:editable="false"
                android:hint="Manifast"
                android:inputType="none"
                android:maxLines="10"
                android:text=""
                android:textColorHint="?colorPrimaryDark"
                android:textIsSelectable="true"
                android:textSize="18.0sp"
                tools:ignore="Deprecated" />
        </com.google.android.material.textfield.TextInputLayout>
    </ScrollView>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="?colorPrimaryDark"
        android:gravity="right|center_vertical"
        android:orientation="horizontal"
        android:paddingTop="10.0dip"
        android:paddingRight="18.0dip"
        android:paddingBottom="10.0dip"
        tools:ignore="RtlHardcoded">

        <TextView
            android:id="@+id/text_info_ok"
            android:layout_width="@dimen/abc_alert_dialog_button_dimen"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15.0dip"
            android:gravity="right|center_vertical"
            android:text="@string/common_word_ok"
            android:textColor="#ffffffff"
            android:textStyle="bold" />
    </LinearLayout>
</LinearLayout>
